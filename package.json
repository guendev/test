{
  "name": "it-square",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build:env": "sed 's/=.*/=/' .env > .env.example",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "apollo:codegen": "npx apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --tagName=gql --includes=./src/apollo/smileeye/**/*.ts --globalTypesFile=__generated__/smileeyeTypes.ts",
    "apollo:download": "npx apollo client:download-schema --endpoint=https://v2-be.smileeye.edu.vn/graphql schema.graphql",
    "restart": "pm2 restart it-square",
    "stop": "pm2 stop it-square"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@formkit/auto-animate": "^1.0.0-beta.1",
    "@nguyenshort/vue3-loading-indicator": "^0.0.7",
    "@vue/apollo-composable": "^4.0.0-alpha.17",
    "@vueuse/core": "^9.1.0",
    "@vueuse/firebase": "^9.1.0",
    "animejs": "^3.2.1",
    "ant-design-vue": "^3.2.11",
    "axios": "^0.27.2",
    "dayjs": "^1.11.2",
    "firebase": "^9.9.2",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.8.2",
    "pinia": "^2.0.14",
    "string-template": "^1.0.0",
    "vue": "^3.2.37",
    "vue-cookies": "^1.8.1",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@iconify-json/ic": "^1.1.8",
    "@types/animejs": "^3.1.4",
    "@types/eslint": "^8.4.3",
    "@types/node": "^18.7.1",
    "@types/shelljs": "^0.8.11",
    "@types/string-template": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "postcss": "^8.4.16",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.0",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-inspect": "^0.6.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vue-tsc": "^0.38.4"
  }
}
